#include <stdio.h>

int main (int argc, char* argv[])
{
    printf("program name: %s\n", argv[0]);

   if( argc == 3 ) {
      printf("The arguments supplied is %s\n%s\n", argv[1], argv[2]);
   }
   else if( argc > 3 ) {
      printf("Too many arguments supplied.\n");
   }
   else {
      printf("One argument expected.\n");
   }
}


